</header>

    <!-- Banner -->
    <div class="heading-page header-text">
      <section class="page-heading" >
           
        <div class="container">
          <div class="row">
            <div class="col-lg-12">
              <div class="text-content">
                <h4>ADMIN PANEL</h4>
                <h2>PASSWORD RESET</h2>
              </div>
            </div>
          </div>
        </div>
      </section>
    </div>

    <!-- Banner -->


<div class="site-wrap">
    
      
    <div class="py-5">
      <div class="container">
        <div class="row">
          
        </div>
      </div>
    </div>  

   
      <div class="container">
        <div class="row">
                @if(TempData["AlertMessage"] != null)
                {
                     <div class="col-md-4 alert alert-success">
                @TempData["AlertMessage"] </div>
                }
           
      
          <div class="col-md-6 pl-12 ">
             <!--asp-page="/Home"--->


            <form method="post">
              
              <div class="p-5 p-lg-5 border">
             
                <div class="form-group row">
                   <div class="col-md-12">
                       <label for="c_pass" class="text-black" style="display:block; color:rgb(47, 58, 77); font-family:Poppins ExtraBold;">What group is this? <span class="text-danger"></span></label>
                       <br>
                                    <label for="c_pass" style="color:rgb(47, 58, 77); font-family:Poppins ExtraBold;">Security Answer <span class="text-danger">*</span></label>
                    <input type="text" class="form-control" id="inputString" name="inputString" placeholder="">
                  </div>
                  <div class="col-md-12" >
                    <label for="c_email" class="text-black" style="color:rgb(47, 58, 77); font-family:Poppins ExtraBold;">New Password <span class="text-danger">*</span></label>
                                    <input type="password" class="form-control" id="password" name="password" placeholder="" pattern="(?=.*\d)(?=.*[a-z])(?=.*[A-Z]).{8,}" title="Must: Be at least 8 characters in length.
                    Include at least one upper case letter.
                    Include at least one number.
                    Include at least one special character." required>
                  </div>
                </div>
                <div class="form-group row">
                 
                   <div class="col-md-12" >
                                    <label for="c_email" class="text-black" style="color:rgb(47, 58, 77); font-family:Poppins ExtraBold;">Confirm Password <span class="text-danger">*</span></label>
                                    <input type="password" class="form-control" id="cpassword" name="cpassword" placeholder="" pattern="(?=.*\d)(?=.*[a-z])(?=.*[A-Z]).{8,}" title="Must: Be at least 8 characters in length.
                    Include at least one upper case letter.
                    Include at least one number.
                    Include at least one special character." required>
                  </div>
                </div>
                
                <div class="form-group row">
                  <div class="col-lg-12">
                                    <input type="submit" class="btn btn-primary btn-lg btn-block" value="submit" style="color:rgb(173, 136, 125); font-family:Poppins ExtraBold; background-color: #fbc1b4; border-color: #fbc1b4;">
                  </div>

                 
                </div>
              </div> 
            </form>
          </div>

        
        </div>
      </div>
   </div>
   

         
    


    

    <!-- Bootstrap -->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Scripts -->
    <script src="assets/js/custom.js"></script>
    <script src="assets/js/owl.js"></script>
    <script src="assets/js/slick.js"></script>
    <script src="assets/js/isotope.js"></script>
    <script src="assets/js/accordions.js"></script>

    <script language = "text/Javascript">
      cleared[0] = cleared[1] = cleared[2] = 0;
      function clearField(t){
      if(! cleared[t.id]){
          cleared[t.id] = 1;
          t.value='';
          t.style.color='#fff';
          }
      }

      document.querySelector('.submit').onclick = function(){
        password = document.querySelector('.password').value,
        cpassword = document.querySelector('.cpassword').value;

        if(password != cpassword){
            alert("Password didn't match")
            return false
        }

        else if (password == cpassword){
            alert("Registration Sucessful")
        }
        return true
    }
    </script>
    <script>
        setInterval(function () {
            $.ajax({
                url: "/managecode?handler=Expiration",
                type: "GET",
                success: function (result) {
                    console.log(result);
                    
                },
                error: function (xhr, status, error) {
                    console.log("Error: " + error);
                }
            });
        }, 1000);



    </script>
  </body>

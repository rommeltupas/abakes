@page
@model abakes.Pages.AdminPanelModel
@{
    ViewData["Title"] = "Admin Panel";
}
<!doctype html>
<html lang="en">

<style>

    .btn-secondary, .btn-secondary:hover, .btn-secondary:active, .btn-secondary:visited {
        background-color: #fbc1b4;
        font-family: 'Poppins', sans-serif;
        font-weight: 700;
        font-style: bold;
    }
</style>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="Mark Otto, Jacob Thornton, and Bootstrap contributors">
    <meta name="generator" content="Hugo 0.104.2">
    <title>Carousel Template · Bootstrap v5.2</title>

    <link rel="canonical" href="https://getbootstrap.com/docs/5.2/examples/carousel/">





    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/css/damarota-blog.css" asp-append-version="true">
    <link rel="stylesheet" href="~/img/" />



    <!-- Custom styles for this template -->
    <link href="~/css/carousel.css" rel="stylesheet">
</head>
  <body>
    
    <nav class="navbar navbar-expand-md navbar-dark fixed-top " style="background-color: #9b846b; color: black !important;"
    <div class="container-fluid">
                <a class="navbar-brand" href="#">
                    <img src="~/img/logo/Logo.png" width="60" height="60" class="d-inline-block align-top" alt="" style="position: relative; left: 20px; right: 10px; margin-right: 20px;">
                </a>
                <a class="navbar-brand" style="color:rgb(47, 58, 77); font-family:Poppins ExtraBold;" href="#">A's Bakes</a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarCollapse">
        <ul class="navbar-nav ms-auto mb-2 mb-md-0">
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" style="color:rgb(47, 58, 77); font-family:Poppins ExtraBold;" href="/Index">HOME</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" style="color:rgb(47, 58, 77); font-family:Poppins ExtraBold;" href="/ProductCustomer?page=1">MENU</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" style="color:rgb(47, 58, 77); font-family:Poppins ExtraBold;" href="/AboutUs">ABOUT US</a>
          </li>
          <li class="nav-item">
                <a class="nav-link"  style="color:rgb(47, 58, 77); font-family:Poppins ExtraBold;" href="/Contact">CONTACT US</a>
          </li>
          <li class="nav-item">
                <a class="nav-link" style="color:rgb(47, 58, 77); font-family:Poppins ExtraBold;" href="/FeedbackCustomer">FEEDBACK</a>
          </li>
                    @if (Model.userconfirm != null)
                    {
                  <li class="nav-item">
                            <a class="nav-link" style="color:rgb(47, 58, 77); font-family:Poppins ExtraBold;" asp-page-handler="LogOut" asp-page="/index">LOGOUT</a>
                  </li>
                  
                    }
                    else
                    {

                    }

        </ul>
      </div>
    </div>
  </nav>
</header>

<main>


         <div class="heading-page">
      <section class="page-heading " style="margin-bottom: 120px">
           
        <div class="container">
          <div class="row">
            <div class="col-lg-12">
              <div class="text-content" >
                <h4>ADMIN PANEL</h4>
                <h2>let’s stay in touch!</h2>
              </div>
            </div>
          </div>
        </div>
      </section>
    </div>


    <!-- ADMIN PANEL-->
    
        <div class="container" style=" border: 3px solid #99846c; border-radius: 10px; padding-top: 120px; padding-bottom:120px; padding-left:120px; background-color: #fbebeb;">

            <!-- Three columns of text below the carousel -->
            <div class="row" style="padding-right: 6%">
                <div class="col">
                    <h1 class="fw-normal"><a class="btn btn-secondary btn-lg" style="background-color: #fbc1b4;  color:rgb(173, 136, 125); font-family:Poppins ExtraBold;" " href="/ProductList"> MANAGE PRODUCT</a></h1>
          
                </div><!-- /.col-lg-4 -->

                <div class="col">
                    <h1 class="fw-normal"><a class="btn btn-secondary btn-lg" style="background-color: #fbc1b4; color:rgb(173, 136, 125); font-family:Poppins ExtraBold;" " href="/FeedbackList"> MANAGE FEEDBACK</a></h1>
                
                </div><!-- /.col-lg-4 -->
                <div class="col">
                    <h1 class="fw-normal"><a class="btn btn-secondary btn-lg" style="background-color: #fbc1b4; color:rgb(173, 136, 125); font-family:Poppins ExtraBold;" href="/managecode"> MANAGE CODE</a></h1>
                
                </div><!-- /.col-lg-4 -->
                <div class="col">
                    <h1 class="fw-normal"><a class="btn btn-secondary btn-lg" style="background-color: #fbc1b4; color:rgb(173, 136, 125); font-family:Poppins ExtraBold;" href="/OrderList"> MANAGE ORDER</a></h1>
                
                </div><!-- /.col-lg-4 -->
                <div class="col">
                    <h1 class="fw-normal"><a class="btn btn-secondary btn-lg" style="background-color: #fbc1b4; color:rgb(173, 136, 125); font-family:Poppins ExtraBold;" " href="/ChangePassword"> CHANGE PASSWORD</a></h1>
                </div><!-- /.col-lg-4 -->
            </div><!-- /.row -->
            </div>

    </main>
    
 

    <script src="../assets/dist/js/bootstrap.bundle.min.js"></script>
    <script>
        setInterval(function () {
            $.ajax({
                url: "/managecode?handler=Expiration",
                type: "GET",
                success: function (result) {
                    console.log(result);
                    // handle the response here
                },
                error: function (xhr, status, error) {
                    console.log("Error: " + error);
                }
            });
        }, 1000);



    </script>

</body>
</html>
